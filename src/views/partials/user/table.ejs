    <table class="table table-borderless">
        <thead class="text-light " style="background-color: rgb(76,175,80);">
            <tr>
                <th>ID</th>
                <th>Nombre</th>
                <th>Contrase√±a</th>
            </tr>
        </thead>
        <tbody class="text-light ">
            <% data.forEach(item => { %>
            <tr>
                <td>
                    <%= item.idUsua %>
                </td>
                <td>
                    <%= item.nombreUsua %>
                </td>
                <td>
                    <%= item.contraUsua %>
                </td>
            </tr>
            <% }) %>
        </tbody>

    </table>
</div>
